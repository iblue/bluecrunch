GEN=ruby ./gen.rb

# These are target files, to be generated by pack.rb
FFT_TO_INT_SOURCES=fft_to_int7.c fft_to_int8.c fft_to_int9.c fft_to_int10.c fft_to_int11.c fft_to_int12.c fft_to_int13.c fft_to_int14.c fft_to_int15.c fft_to_int16.c fft_to_int17.c fft_to_int18.c fft_to_int19.c fft_to_int20.c fft_to_int21.c fft_to_int22.c
INT_TO_FFT_SOURCES=int_to_fft7.c int_to_fft8.c int_to_fft9.c int_to_fft10.c int_to_fft11.c int_to_fft12.c int_to_fft13.c int_to_fft14.c int_to_fft15.c int_to_fft16.c int_to_fft17.c int_to_fft18.c int_to_fft19.c int_to_fft20.c int_to_fft21.c int_to_fft22.c

all: $(FFT_TO_INT_SOURCES) $(INT_TO_FFT_SOURCES)
	@true # placeholder to prevent CC invocation

%.c:
	$(GEN) $@

clean:
	rm -f $(FFT_TO_INT_SOURCES)
	rm -f $(INT_TO_FFT_SOURCES)
